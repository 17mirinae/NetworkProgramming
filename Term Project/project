int check_bingo() {
        int i, j, win[PEOPLE]={0,0};
        int count;

        for(j = 0; j < PEOPLE; j++) {
                count=0;
                for(i = 0; i < BOARD_SIZE; i++) // ê°€ë¡œ, ì„¸ë¡œ
                {
                        if(board[j][i][0]==0 && board[j][i][1]==0 && board[j][i][2]==0 && board[j][i][3]==0 && board[j][i][4]==0) //ê°€ë¡œ ê²€ì‚¬
                                count++;
                        if(board[j][0][i]==0 && board[j][1][i]==0 && board[j][2][i]==0 && board[j][3][i]==0 && board[j][4][i]==0) //ì„¸ë¡œ ê°ì‚¬
                                count++;
                }

                // ëŒ€ê°ì„  ê²€ì‚¬
                if(board[j][0][0]==0 && board[j][1][1]==0 && board[j][2][2]==0 && board[j][3][3]==0 && board[j][4][4]==0)
                        count++;
                if(board[j][0][4]==0 && board[j][1][3]==0 && board[j][2][2]==0 && board[j][3][1]==0 && board[j][4][0]==0)
                        count++;
                if(count==WIN) {
                        win[j]=1;
                }
        }

        if(win[0]&&win[1]) { // ë¹„ê¹€
                return 3;
        } else if(win[0]) { // ì´ê²¼ìŒ
                return 1;
        } else if(win[1]) { // ì¡ŒìŒ
                return 2;
        } else {
                return 0;
        }
}

int remove_from_board(int turn, char num[]) { // ì…ë ¥ë°›ì€ ìˆ«ìë‘ ë¹„êµí•´ ìœ íš¨í•˜ë©´ 0ìœ¼ë¡œ ë°”ê¿” ë„£ëŠ” ê¸°ëŠ¥
    int number = atoi(num);
    int inx;
    for(inx = 0;inx < BOARD_SIZE*BOARD_SIZE;inx++){  // compare
        if(board[turn][0][inx] == number){
            board[turn][0][inx] = 0;
            break;
        }
    }
    return inx==25||number<1? 0:1;
}

void game_set(){ // ë³´ë“œì— ë°ì´í„°ë¥¼ ë„£ê³  í•´ë‹¹ ë³´ë“œì˜ ë°ì´í„°ë¥¼ ë¬¸ìì—´ë¡œ ë³€í™˜í•´ì„œ ë³´ë‚´ì£¼ëŠ” ê¸°ëŠ¥
    int a;
    for(a = 0; a < num_chat ; a++){
            make_board(a);
            board_to_string(a);
            puts("");
    }
    user_turn = 0;
}

void addClient(int s, struct sockaddr_in *newcliaddr) { // ì—°ê²° ì†Œì¼“ í•˜ë‚˜ ì¶”ê°€í•˜ëŠ” ê¸°ëŠ¥
        char buf[20];
        inet_ntop(AF_INET, &newcliaddr->sin_addr, buf, sizeof(buf));
        printf("New Client : %s\n", buf);

        clisock_list[num_chat] = s;
        num_chat++;
}

void removeClient(int s) { // ì—°ê²° ì¤‘ì¸ ì†Œì¼“ë“¤ ì¤‘ ê±´ë„¤ë°›ì€ ì†Œì¼“ë²ˆí˜¸ ì†Œì¼“ ì œê±°
        send(clisock_list[s],EXIT_STRING,strlen(EXIT_STRING),0);
        close(clisock_list[s]);
        if(s != num_chat - 1)
                clisock_list[s] = clisock_list[num_chat - 1];
        num_chat--;
        printf("ì±„íŒ… ì°¸ê°€ì 1ëª… íƒˆí‡´. í˜„ì¬ ì°¸ê°€ì ìˆ˜ : %d\n", num_chat);
}

int is_nonblock(int sockfd) {
        int val;
        // ê¸°ì¡´ì˜ í”Œë˜ê·¸ ê°’ì„ ì–»ì–´ì˜¨ë‹¤.
        val = fcntl(sockfd, F_GETFL, 0);
        // ë„Œë¸”ë¡ ëª¨ë“œì¸ì§€ í™•ì¸
        if(val & O_NONBLOCK)
                return 0;
        return -1;
}

int set_nonblock(int sockfd) {
        int val;
        val = fcntl(sockfd, F_GETFL, 0);
        if(fcntl(sockfd, F_SETFL, val | O_NONBLOCK) == -1)
                return -1;
        return 0;
}

int getmax() {
        int max = listen_sock;
        int i;

        for(i = 0;i < num_chat;i++)
                if(clisock_list[i] > max)
                        max = clisock_list[i];

        return max;
}

int tcp_listen(int host, int port, int backlog) {
        int sd;
        struct sockaddr_in servaddr;

        sd = socket(AF_INET, SOCK_STREAM, 0);
        if(sd == -1) {
                perror("socket fail");
                exit(1);
        }

        bzero((char *)&servaddr, sizeof(servaddr));
        servaddr.sin_family = AF_INET;
        servaddr.sin_addr.s_addr = htonl(host);
        servaddr.sin_port = htons(port);

        if(bind(sd, (struct sockaddr *)&servaddr, sizeof(servaddr)) < 0) {
                perror("bind fail");
                exit(1);
        }

        listen(sd, backlog);
        return sd;
}

void make_board(int user_id) { // ë³´ë“œì— ë°ì´í„°ë¥¼ ì§‘ì–´ë„£ëŠ” ê¸°ëŠ¥
        int i, j, temp;
        int rd;
        srand(time(NULL));
        for(i =0; i< BOARD_SIZE*BOARD_SIZE ; i++){
                board[user_id][0][i] = i+1;
        }

        for(i=0; i<1000; i++){
                rd = (rand()+(user_id+73))%25;
                temp =board[user_id][0][0];
                board[user_id][0][0] = board[user_id][0][rd];
                board[user_id][0][rd] = temp;
        }
}

int board_to_string(int cli_id) { // ë³´ë“œì˜ ë°ì´í„°ë¥¼ ë¬¸ìì—´ë¡œ ë³€í™˜í•´ì„œ ë³´ë‚´ëŠ” ê¸°ëŠ¥
        int i = 0;
        char temp[10];
        char msg[MAXLINE] = "";
        for(;i < BOARD_SIZE*BOARD_SIZE;i++) {
                sprintf(temp, "%d", board[cli_id][0][i]);
                strcat(msg, temp);
                if(i != (BOARD_SIZE * BOARD_SIZE)-1) {
                        strcat(msg, ",");
                }
        }

        printf("%s", msg);
        send(clisock_list[cli_id], msg, strlen(msg), 0);
	return 1;
}
